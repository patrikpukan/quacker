    "apollo-upload-client": "^18.0.1",
    "cross-fetch": "^4.0.0",
    "graphql-tag": "^2.12.6",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.6.2",
    "zod": "^3.22.5"
