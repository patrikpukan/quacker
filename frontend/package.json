    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
